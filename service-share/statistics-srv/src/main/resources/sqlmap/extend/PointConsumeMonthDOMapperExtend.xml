<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lawu.eshop.statistics.srv.mapper.extend.PointConsumeMonthDOMapperExtend" >
    
    <select id="getReportDatePointConsumeDaily" resultType="com.lawu.eshop.statistics.srv.domain.extend.ReportNewDateDOView" >
         select gmt_report as gmtReport from report_point_consume_daily  ORDER BY ID DESC LIMIT 0,1
    </select>

	<select id="getReportDatePointConsumeMonth" resultType="com.lawu.eshop.statistics.srv.domain.extend.ReportNewDateDOView" >
         select gmt_report as gmtReport from report_point_consume_month  ORDER BY ID DESC LIMIT 0,1
    </select>

</mapper>
